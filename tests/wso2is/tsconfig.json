{
  "compilerOptions": {
      "esModuleInterop": true,
      "module": "esnext",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "jsx": "react",
      "resolveJsonModule": true,
      "noLib": false,
      "baseUrl": ".",
      "declaration": true,
      "experimentalDecorators": true,
      "lib": [
          "dom",
          "es6",
      ],
      "outDir": "dist",
      "target": "es5",
      "types": [
          "cypress",
          "node"
       ],
      "sourceMap": false
  },
  "include": [
      "cypress/e2e",
      "cypress/fixtures",
      "cypress/support"
  ],
  "compileOnSave": false,
  "exclude": [
      "node_modules",
      "cypress/reports",
      "logs"
  ]
}
